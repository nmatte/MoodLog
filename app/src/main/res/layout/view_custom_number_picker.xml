<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/chart_edit_entry_width_m" android:layout_height="@dimen/chart_cell_height_m"
    android:background="@color/white">


    <ImageButton
        style="@style/CustomNumberPicker.MinusButton"
        android:id="@+id/minusButton"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:text="0"
        android:id="@+id/textCellView"
        android:gravity="center"
        android:layout_centerInParent="true"/>



    <ImageButton
        style="@style/CustomNumberPicker.PlusButton"
        android:id="@+id/plusButton"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"/>
        />
</RelativeLayout>
