<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/chart_cell_height_m"
    android:background="@color/white">



    <TextView
        android:id="@+id/textCellView"
        android:layout_width="@dimen/chart_cell_width_m"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:gravity="center"
        android:text="0"/>

    <ImageButton
        android:id="@+id/minusButton"
        style="@style/CustomNumberPicker.MinusButton"
        android:layout_alignRight="@+id/plusButton"
        android:layout_centerVertical="true"
        />

    <ImageButton
        android:id="@+id/plusButton"
        style="@style/CustomNumberPicker.PlusButton"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"/>
</RelativeLayout>
