<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="NumPicker">
        <attr name = "label" format="string"/>
        <attr name = "defaultNum" format="integer"/>
        <attr name = "max" format="integer"/>
    </declare-styleable>
    <declare-styleable name="TextCellView">
        <attr name = "text" format="string"/>
        <attr name = "horizontal_alignment" format="string"/>
        <attr name = "vertical_alignment" format="string"/>
    </declare-styleable>


    <declare-styleable name="ChartColumn">
        <attr name="mode_edit" format="boolean"/>
        <attr name="mode_label" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="VerticalText">
        <attr name="vertical_text" format="string"/>
    </declare-styleable>
</resources>
