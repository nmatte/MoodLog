<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomNumberPicker">
        <attr name = "label" format="string"/>
        <attr name = "defaultNum" format="integer"/>
        <attr name = "max" format="integer"/>
    </declare-styleable>
    <declare-styleable name="TextCellView">
        <attr name = "text" format="string"/>
        <attr name = "horizontal_alignment" format="string"/>
        <attr name = "vertical_alignment" format="string"/>
    </declare-styleable>
    <declare-styleable name="BoolItemList">
        <attr name="style_chart" format="boolean"/>
        <attr name="is_enabled" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ChartColumn">
        <attr name="view_mode" format="enum">
            <enum name="edit_entry" value="0"/>
            <enum name="read_entry" value="1"/>
            <enum name="label" value="2"/>
        </attr>
    </declare-styleable>

</resources>
